{
  "root": true,
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "project": [ "./tsconfig.json" ]
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off"
  }
}
